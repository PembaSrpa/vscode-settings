    "blade": "html",
    "vue-html": "html",
    "vue": "html",
    "react": "html",
    "javascript": "html"
  },
  /**
   * Prettier
   **/ "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
